enabled:
  _mandatory: true
  _type: bool
  _doc: boolean parameter

disabled:
  _mandatory: true
  _type: bool
  _doc: boolean parameter

mode:
  _mandatory: true
  _type: string
  _doc: string parameter

param_int:
  _mandatory: true
  _type: int
  _doc: some integer param

param_double:
  _mandatory: true
  _type: double
  _doc: some double param

